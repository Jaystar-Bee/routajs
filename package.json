{
  "name": "routajs",
  "version": "1.0.2",
  "description": "A lightweight, framework-agnostic router plugin",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "lint": "eslint src/**",
    "release": "release-it"
  },
  "keywords": [
    "router",
    "routing",
    "history",
    "spa",
    "ssr",
    "typescript"
  ],
  "author": "Jaystar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jaystar-Bee/routajs.git"
  },
  "bugs": {
    "url": "https://github.com/Jaystar-Bee/routajs/issues"
  },
  "homepage": "https://github.com/Jaystar-Bee/routajs#readme",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "release-it": "^19.2.4",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}